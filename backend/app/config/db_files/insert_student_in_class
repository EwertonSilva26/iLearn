DELIMITER $$
CREATE PROCEDURE insert_student_in_class (idStudent INT, classCode varchar(100))
BEGIN
	DECLARE classId INT;
    
    SELECT id_class FROM tb_class WHERE class_code = classCode INTO classId;
	
    SET FOREIGN_KEY_CHECKS=0;
    INSERT tb_student_class (id_student, id_class) values (idStudent, classId);
END $$
DELIMITER ;


CALL insert_student_in_class(5, 'a5o1bo7qq4');